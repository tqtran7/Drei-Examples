{
  "include": [
      "./src/**/*"
  ],
  "compilerOptions": {
      "strict": true,
      "esModuleInterop": true,
      "lib": [
          "dom",
          "es2015",
          "ESNext"
      ],
      "target": "ES5",
      "jsx": "react-jsx",
      "downlevelIteration": true,
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "moduleResolution": "node",
      "baseUrl": "src",
      "paths": {
        "@api/*": ["src/api/*"],
        "@data/*": ["src/data/*"],
        "@assets/*": ["src/assets/*"],
        "*": ["types/*.d.ts"],
      }
  }
}